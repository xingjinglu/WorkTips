   "source": [
    "import torch\n",
    "def f(x)->torch.tensor:\n",
    "    result = 1\n",
    "    for ii in x:\n",
    "        if ii.item() > 0: result = ii * result\n",
    "    return result\n",
    "\n",
    "x = torch.tensor([0.3071, 1.1043, 1.3605, -0.3471], requires_grad = True)\n",
    "y = f(x)\n",
    "y.backward()\n",
    "print(x.grad)"
   ]
